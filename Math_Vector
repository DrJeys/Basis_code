# Calculate the angle of the two vector

a=np.array([1,1,1])
b=np.array([2,2,2])
def angle_vector(a,b):
  cos_ab=np.dot(a,b)/(np.linalg.norm(a)*np.linalg.norm(b))
  angle_in_radians = np.arccos(cos_ab)
  angle_in_degrees = np.degrees(angle_in_radians)
print(angle_vector(a,b))

# dot product
dot=np.dot(a,b)

#outer product
outer=np.outer(a,b)

#hadamard product
multiply=np.multiply(a,b)
